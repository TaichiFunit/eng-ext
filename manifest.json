{
  "manifest_version": 3,
  "name": "Download Button Data",
  "version": "1.0",
  "permissions": [
    "downloads",
    "activeTab",
    "tabs",
    "scripting",
    "https://tv.eslpod.com/*"
    
  ],
  "background": {
    "service_worker": "background.js"
  },
  "commands": {
    "trigger_download": {
      "suggested_key": {
        "default": "Ctrl+Shift+Y"
      },
      "description": "Trigger download from active tab"
    }
  }
}